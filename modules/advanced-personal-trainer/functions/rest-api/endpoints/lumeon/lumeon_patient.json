{
    "resourceType":"Patient",
    "extension":[
        {
            "url":"http://fhir.qinec.com/dstu2/patient/VIP",
            "valueBoolean":false
        }
    ],
    "name":[
        {
            "use":"official",
            "family":[
                "{patient_surname}"
            ],
            "given":[
                "{patient_firstname}"
            ],
            "prefix":[
                "{patient_title}"
            ]
        }
    ],
    "telecom":[
        {
            "use":"work",
            "system":"phone",
            "value":"{patient_phone_work}"
        },
        {
            "use":"mobile",
            "system":"phone",
            "value":"{patient_phone_mobile}"
        },
        {
            "use":"home",
            "system":"email",     
            "value":"{patient_email}"
        }
    ],
    "gender":"{patient_gender}",
    "birthDate":"{patient_dob}",
    "address":[
        {
            "use":"home",
            "text":"{patient_addr_1} {patient_addr_2} {patient_addr_city} {patient_addr_county} {patient_addr_postcode} {patient_addr_country}",
            "line":[
                "",
                "{patient_addr_1}",
                "{patient_addr_2}",
                "",
                ""
            ],
            "city":"{patient_addr_city}",
            "state":"{patient_addr_county}",
            "postalCode":"{patient_addr_postcode}",
            "country":"{patient_addr_country}"
        }
    ]
}